client
remote 87-1-fr.cg-dialup.net 443
dev tun 
proto udp
auth-user-pass /etc/openvpn/provider/openvpn-credentials.txt


resolv-retry infinite 
redirect-gateway def1
persist-key
persist-tun
nobind
cipher AES-256-CBC
ncp-disable
auth SHA256
ping 5
ping-exit 60
ping-timer-rem
explicit-exit-notify 2
script-security 2
remote-cert-tls server
route-delay 5
verb 4


ca /etc/openvpn/provider/ca.crt

cert /etc/openvpn/provider/client.crt

key /etc/openvpn/provider/client.key
